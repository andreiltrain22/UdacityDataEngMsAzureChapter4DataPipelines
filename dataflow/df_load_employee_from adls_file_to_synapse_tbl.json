{
	"name": "df_load_employee_from adls_file_to_synapse_tbl",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_adls_Employee",
						"type": "DatasetReference"
					},
					"name": "srcAdlsCsvEmployee"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_synapse_Employee",
						"type": "DatasetReference"
					},
					"name": "tgtSynapseTblEmployee"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> srcAdlsCsvEmployee",
				"srcAdlsCsvEmployee sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          EmployeeID,",
				"          LastName,",
				"          FirstName",
				"     )) ~> tgtSynapseTblEmployee"
			]
		}
	}
}